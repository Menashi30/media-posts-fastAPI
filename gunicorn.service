[Unit]
Description=demo fastapi application
After=network.target

[Service]
User=menashi
Group=menshi
WorkingDirectory=/home/menashi/app/src/
Environment="PATH=/home/menashi/app/venv/bin"
EnvironmentFile=/home/menashi/.env
ExecStart=/home/menashi/app/venv/bin/gunicorn -w 4 -k uvicorn.workers.UvicornWorker app.main:app --bind 0.0.0.0:8000

[Install]
WantedBy=multi-user.target